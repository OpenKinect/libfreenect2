# - Config file for the freenect2 package
# It defines the following variables
#  freenect2_INCLUDE_DIRS - include directories for stdng
#  freenect2_LIBRARIES    - libraries to link against

# Compute paths
GET_FILENAME_COMPONENT(freenect2_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
SET(freenect2_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
INCLUDE("${freenect2_CMAKE_DIR}/freenect2Targets.cmake")

# These are IMPORTED targets created by freenect2Targets.cmake
SET(freenect2_LIBRARIES freenect2)
